<h1>Listing message_actions</h1>

<table>
  <tr>
    <th>Message</th>
  </tr>

<% for message_action in @message_actions %>
  <tr>
    <td><%=h message_action.message %></td>
    <td><%= link_to 'Show', message_action %></td>
    <td><%= link_to 'Edit', edit_message_action_path(message_action) %></td>
    <td><%= link_to 'Destroy', message_action, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New message_action', new_message_action_path %>
